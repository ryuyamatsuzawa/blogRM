<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <title>ユーザリスト</title>
</head>
<body>
    <p><a href="/create">ユーザ追加</a></p>
    <table>
        <tr>
            <th>Id</th>
            <th>名前</th>
            <th>Email</th>
            <th>パスワード</th>
            <th>更新</th>
            <th>削除</th>
        </tr>
        <% users.forEach(function (value) { %>
        <tr>
            <td><%= value.id %></td>
            <td><%= value.name %></td>
            <td><%= value.email %></td>
            <td><%= value.password %></td>
            <td><a href="/edit/<%= value.id %>">更新</a></td>
            <td><a href="/delete/<%= value.id %>">削除</a></td>
        </tr>
        <% }); %>
    </table>
</body>
</html>